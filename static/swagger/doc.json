{
  "swagger": "2.0",
  "info": {
    "title": "Back End Go API",
    "description": "This is an API for DevAPI (GO+PostgreSQL)",
    "contact": {
      "name": "Nizar Developer",
      "url": "https://nizar.id/",
      "email": "nizar.aplikasi@gmail.com"
    },
    "version": "1.0"
  },
  "host": "",
  "basePath": "/api/v1",
  "schemes": [],
  "paths": {
    "/auth/signin": {
      "post": {
        "description": "Signin with username and password",
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "tags": ["auth"],
        "summary": "Signin",
        "parameters": [
          {
            "description": "Login payload with Base64 encoded username:password",
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/dtos.LoginFormRequestDto"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful signin",
            "schema": {
              "$ref": "#/definitions/dtos.ApiResponse"
            }
          }
        }
      }
    },
    "/auth/refresh": {
      "post": {
        "security": [{ "ApiKeyAuth": [] }],
        "description": "Refresh token",
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "tags": ["auth"],
        "summary": "Refresh",
        "parameters": [
          {
            "description": "Refresh token DTO",
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/dtos.RefreshTokenDto"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful refresh token",
            "schema": {
              "$ref": "#/definitions/dtos.ApiResponse"
            }
          }
        }
      }
    },
    "/user/create": {
      "post": {
        "security": [{ "ApiKeyAuth": [] }],
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "tags": ["user"],
        "summary": "Create User",
        "parameters": [
          {
            "description": "New user data",
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/dtos.AddUserDto"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful create user",
            "schema": {
              "$ref": "#/definitions/dtos.ApiResponse"
            }
          }
        }
      }
    },
    "/user/me": {
      "get": {
        "security": [{ "ApiKeyAuth": [] }],
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "tags": ["user"],
        "summary": "Get user info",
        "responses": {
          "200": {
            "description": "Successful user me",
            "schema": {
              "$ref": "#/definitions/dtos.ApiResponse"
            }
          }
        }
      }
    },
    "/user/list": {
      "get": {
        "security": [{ "ApiKeyAuth": [] }],
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "tags": ["user"],
        "summary": "List all users",
        "description": "Get a list of all users in the system.",
        "responses": {
          "200": {
            "description": "Successful user list",
            "schema": {
              "$ref": "#/definitions/dtos.UserListResponse"
            }
          }
        }
      }
    },
    "/user/{id}": {
      "delete": {
        "security": [{ "ApiKeyAuth": [] }],
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "tags": ["user"],
        "summary": "Deactivate User",
        "parameters": [
          {
            "type": "string",
            "description": "User ID",
            "name": "id",
            "in": "path",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "Successful user deactivate",
            "schema": {
              "$ref": "#/definitions/dtos.ApiResponse"
            }
          }
        }
      }
    },
    "/user/change-password": {
      "post": {
        "security": [{ "ApiKeyAuth": [] }],
        "description": "Change password",
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "tags": ["user"],
        "summary": "Change Password",
        "parameters": [
          {
            "description": "Change password data",
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/dtos.ChangePasswordDto"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful change password",
            "schema": {
              "$ref": "#/definitions/dtos.ApiResponse"
            }
          }
        }
      }
    },
    "/user/fetch": {
      "get": {
        "security": [{ "ApiKeyAuth": [] }],
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "tags": ["user"],
        "summary": "Fetch users",
        "parameters": [
          {
            "type": "string",
            "name": "last_item",
            "in": "query"
          },
          {
            "type": "integer",
            "default": 10,
            "name": "length",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful fetch users",
            "schema": {
              "$ref": "#/definitions/dtos.ApiResponse"
            }
          }
        }
      }
    }
  },
  "definitions": {
    "dtos.ApiResponse": {
      "type": "object",
      "properties": {
        "data": {},
        "message": {
          "type": "string"
        },
        "success": {
          "type": "boolean"
        }
      }
    },
    "dtos.LoginFormRequestDto": {
      "type": "object",
      "properties": {
        "login_form": {
          "type": "string",
          "format": "base64"
        }
      }
    },
    "dtos.ChangePasswordDto": {
      "type": "object",
      "properties": {
        "username": {
          "type": "string"
        },
        "password": {
          "type": "string"
        }
      }
    },
    "dtos.AddUserDto": {
      "type": "object",
      "required": [
        "username",
        "password",
        "fullname",
        "notelp",
        "orgname",
        "role"        
      ],
      "properties": {
        "username": {
          "type": "string"
        },
        "password": {
          "type": "string",
          "minLength": 8
        },
        "fullname": {
          "type": "string"         
        },
        "notelp": {
          "type": "string"         
        },
        "orgname": {
          "type": "string"          
        },
        "role": {
          "type": "string"
        }        
      }
    },
    "dtos.RefreshTokenDto": {
      "type": "object",
      "properties": {
        "refresh_token": {
          "type": "string"
        }
      }
    },
    "dtos.UserListResponse": {
      "type": "object",
      "properties": {
        "success": {
          "type": "boolean"
        },
        "message": {
          "type": "string"
        },
        "data": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/dtos.UserItem"
          }
        }
      }
    },
    "dtos.UserItem": {
      "type": "object",
      "properties": {
        "username": {
          "type": "string"
        },
        "fullname": {
          "type": "string"
        },
        "orgname": {
          "type": "string"
        },
        "role": {
          "type": "string"
        }
      }
    }
  },  
  "securityDefinitions": {
    "ApiKeyAuth": {
      "type": "apiKey",
      "in": "header",
      "name": "Authorization",
      "description": "Enter your Bearer token in the format **Bearer &lt;token&gt;**"
    }
  },
  "security": [
    {
      "ApiKeyAuth": []
    }
  ]
}